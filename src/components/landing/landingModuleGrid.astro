---
import GridSection from "./landingModule";
import {
	plmProps,
	cabalProps,
	smileNowProps,
	aboutMeProps,
	quaeProps,
	TraderInteractiveProps,
} from "./landingModuleData";
---

<section id="grid">
	<div id="bio-module">
		<GridSection {...aboutMeProps} client:only="solid-js" />
	</div>
	<div id="trader-interactive-module">
		<GridSection {...TraderInteractiveProps} client:only="solid-js" />
	</div>
	<div id="plm-module">
		<GridSection {...plmProps} client:only="solid-js" />
	</div>
	<div id="cabal-module">
		<GridSection {...cabalProps} client:only="solid-js" />
	</div>
	<div id="smilenow-module">
		<GridSection {...smileNowProps} client:only="solid-js" />
	</div>
	<div id="quae-module">
		<GridSection {...quaeProps} client:only="solid-js" />
	</div>

	<!-- <div id="projects-module">
		<GridSection {...projectsProps} client:load>
			<ProjectsPreview client:load />
		</GridSection>
	</div> -->
</section>
